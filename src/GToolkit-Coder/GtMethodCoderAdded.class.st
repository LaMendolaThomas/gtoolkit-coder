Class {
	#name : #GtMethodCoderAdded,
	#superclass : #GtCoderMethodAnnouncement,
	#instVars : [
		'methodCoder'
	],
	#category : #'GToolkit-Coder-Event'
}

{ #category : #accessing }
GtMethodCoderAdded >> methodCoder [
	^ methodCoder
]

{ #category : #accessing }
GtMethodCoderAdded >> methodCoder: aMethodCoder [
	methodCoder := aMethodCoder
]
